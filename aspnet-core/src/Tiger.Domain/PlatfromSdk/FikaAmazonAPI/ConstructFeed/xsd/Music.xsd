                    <xsd:annotation>
                        <xsd:documentation>This field is required for records (MediaType
							lp_record).</xsd:documentation>
                    </xsd:annotation>
                </xsd:element>
                <xsd:element maxOccurs="5" minOccurs="0" name="Format" type="MusicFormatType"/>
                <xsd:element maxOccurs="2" minOccurs="0" name="DVDRegion" type="MusicDVDRegionType"/>
